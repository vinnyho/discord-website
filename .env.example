# Stripe API Keys (get from https://dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Stripe Price ID (create a product in Stripe Dashboard)
STRIPE_PRICE_ID=price_your_price_id_here

# Stripe Webhook Secret (from Stripe Dashboard or CLI)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Your domain
NEXT_PUBLIC_BASE_URL=http://localhost:3001

# Discord OAuth (get from https://discord.com/developers/applications)
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Discord Bot (from Bot section of your Discord app)
DISCORD_BOT_TOKEN=your_bot_token_here
DISCORD_GUILD_ID=your_server_id_here
DISCORD_ROLE_ID=your_premium_role_id_here

# NextAuth
AUTH_SECRET=generate_a_random_secret_here
AUTH_URL=http://localhost:3001
